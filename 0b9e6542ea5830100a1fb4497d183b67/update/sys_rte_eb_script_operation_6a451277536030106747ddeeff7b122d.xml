        <script><![CDATA[(function(batch, output) {
    for (var i = 0; i < batch.length; i++) {
        var a = batch[i].input;
        var b = convertOSDomain(a);
        output[i] = a;
   }
   
   function convertOSDomain(domainName){
      if (!domainName || domainName === '(none)' || domainName === 'No Domain' || !domainName.split) {
            return 'none';
        }

        var parts = domainName.split('.');

        return parts[0].toLowerCase();
   }
   
})(batch, output);
]]></script>
